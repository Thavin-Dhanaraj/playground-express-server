{
    "version": 1,
    "builds": [{ "src": "index.js", "use": "@now/node-server" }],
    "routes": [
      {
        "src": "/users",
        "dest": "/index.js",
        "methods": ["GET"]
      },
      {
        "src": "/users",
        "dest": "/index.js",
        "methods": ["POST"]
      },
      {
        "src": "/users/login",
        "dest": "/index.js",
        "methods": ["POST"]
      },
      {
        "src": "/users/register",
        "dest": "/index.js",
        "methods": ["POST"]
      },
      {
        "src": "/users/:userId",
        "dest": "/index.js",
        "methods": ["GET"]
      },
      {
        "src": "/users/:userId",
        "dest": "/index.js",
        "methods": ["PUT"]
      },
      {
        "src": "/users/:userId",
        "dest": "/index.js",
        "methods": ["DELETE"]
      }
    ]
  }